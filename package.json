{
  "name": "swiftapp",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "uncache": "react-native start --reset-cache",
    "ios": "ENVFILE=.env.prd react-native run-ios",
    "ios:dev": "ENVFILE=.env.dev react-native run-ios",
    "ios:stg": "ENVFILE=.env.stg react-native run-ios",
    "android": "ENVFILE=.env.prd react-native run-android",
    "android:dev": "ENVFILE=.env.dev react-native run-android",
    "android:stg": "ENVFILE=.env.stg react-native run-android",
    "build": "cd android && ENVFILE=.env.prd ./gradlew assembleRelease",
    "build:dev": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease",
    "build:stg": "cd android && ENVFILE=.env.stg ./gradlew assembleRelease",
    "build:bundle": "cd android && ENVFILE=.env.prd ./gradlew bundleRelease",
    "gqlgen": "graphql-codegen",
    "gqlgen:stg": "graphql-codegen --config ./codegen.stg.yml",
    "gqlgen:prd": "graphql-codegen --config ./codegen.prd.yml",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --fix",
    "pod": "cd ios && pod install",
    "pod:i": "cd ios && pod install",
    "pod:i:m1": "cd ios && arch -x86_64 pod install",
    "pod:u": "cd ios && pod install --repo-update",
    "pod:u:m1": "cd ios && arch -x86_64 pod install --repo-update",
    "prepare": "husky install",
    "pod:c:m1": "rm -rf ios/Pods && rm -rf ios/Podfile.lock && yarn pod:i:m1",
    "clean": "rm -rf node_modules && yarn install && rm -rf ios/Pods && rm -rf ios/Podfile.lock && yarn pod:i",
    "clean:m1": "rm -rf node_modules && yarn install && rm -rf ios/Pods && rm -rf ios/Podfile.lock && yarn pod:i:m1",
    "clean:m1:pod": "rm -rf node_modules && yarn install && rm -rf ios/Pods && yarn pod:i:m1",
    "clean:lock": "rm -rf node_modules && rm -rf yarn.lock && yarn install && rm -rf ios/Pods && rm -rf ios/Podfile.lock && yarn pod:i",
    "clean:lock:m1": "rm -rf node_modules && rm -rf yarn.lock && yarn install && rm -rf ios/Pods && rm -rf ios/Podfile.lock && yarn pod:i:m1",
    "bundle:js:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "bundle:js:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@graphql-codegen/fragment-matcher": "^1.17.3",
    "@react-native-async-storage/async-storage": "^1.17.0",
    "@react-native-community/datetimepicker": "^3.0.3",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/toolbar-android": "0.1.0-rc.2",
    "@react-native-firebase/analytics": "^14.5.1",
    "@react-native-firebase/app": "^14.5.1",
    "@react-native-firebase/crashlytics": "^14.5.1",
    "@react-native-firebase/dynamic-links": "^14.5.1",
    "@react-native-firebase/firestore": "^14.5.1",
    "@react-native-firebase/messaging": "^14.5.1",
    "@react-native-firebase/remote-config": "^14.5.1",
    "@react-navigation/bottom-tabs": "^5.5.2",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.5.1",
    "cross-fetch": "^3.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "graphql": "^15.1.0",
    "graphql-tag": "^2.10.3",
    "i18next": "^19.4.5",
    "lodash": "^4.17.19",
    "react": "17.0.1",
    "react-hook-form": "^7.10.1",
    "react-i18next": "^11.7.0",
    "react-native": "0.64.2",
    "react-native-code-push": "^6.2",
    "react-native-collapsible": "^1.5.3",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^6.0.4",
    "react-native-error-boundary": "^1.1.3",
    "react-native-fast-image": "^8.3.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-htmlview": "^0.16.0",
    "react-native-image-crop-picker": "^0.35.1",
    "react-native-image-viewing": "^0.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.31.1",
    "react-native-normalize": "^1.0.1",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "^1.9.0",
    "react-native-render-html": "^6.3.4",
    "react-native-restart": "^0.0.17",
    "react-native-safe-area-context": "^3.0.5",
    "react-native-screens": "^2.8.0",
    "react-native-share": "^5.1.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-store-version": "^1.3.2",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^10.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.14.0",
    "husky": "^7.0.1",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "2.3.2",
    "reactotron-react-native": "^5.0.1"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/_src/core/_modules/*/__tests__/*.test.js"
    ],
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "react-native"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native)"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
